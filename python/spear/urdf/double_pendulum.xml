<robot name="benchmark_torque_revolute">
    <!--
        URDF_FILE: "simple_continuous.xml"
        KEYBOARD_ACTIONS:
          One:
            ADD_ACTION: { "joint_0": 9.8 }
          Two:
            ADD_ACTION: { "joint_0": -10 }
          Three:
            ADD_ACTION: { "joint_0": 0.1 }
          Four:
            ADD_ACTION: { "joint_0": -0.1 }
    -->
    <mujoco>
        <compiler discardvisual="false"/>
    </mujoco>
    <material name="M_Red">
        <texture filename="/UrdfBot/Common/M_Red.M_Red"/>
    </material>
    <material name="M_Green">
        <texture filename="/UrdfBot/Common/M_Green.M_Green"/>
    </material>
    <material name="M_Blue">
        <texture filename="/UrdfBot/Common/M_Blue.M_Blue"/>
    </material>

    <link name="base_link">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0.0 0"/>
            <mass value="100"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
            <material>
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
        </collision>
    </link>

    <link name="link_0">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 -1"/>
            <mass value="1"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -1"/>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
            <material>
                <color rgba="0 1.0 0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -1"/>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
        </collision>
    </link>
    <joint name="joint_0" type="continuous">
        <origin rpy="0 0 0" xyz="1 0 0"/>
        <parent link="base_link"/>
        <child link="link_0"/>
        <axis xyz="1 0 0"/>
        <limit effort="0" velocity="1000"/>
        <dynamics type="torque"/>
    </joint>

    <link name="link_1">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 -1"/>
            <mass value="1"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -1"/>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
            <material name="M_Blue"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -1"/>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
            <material name="M_Blue">
                <color rgba="0 0 1 1"/>
                <texture filename="/UrdfBot/Common/M_Blue.M_Blue"/>
            </material>
        </collision>
    </link>
    <joint name="joint_1" type="continuous">
        <origin rpy="0 0 0" xyz="0 0 -2"/>
        <parent link="link_0"/>
        <child link="link_1"/>
        <axis xyz="1 0 0"/>
        <limit effort="0" velocity="1000"/>
        <dynamics type="torque"/>
    </joint>

